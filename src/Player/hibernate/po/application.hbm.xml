<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE   hibernate-mapping   
     PUBLIC   "-//Hibernate/Hibernate   Mapping   DTD//EN"   
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
     
<hibernate-mapping>
	<class name="Player.hibernate.po.Application" table="application">
		<id name="stuId" type="int">
				<column name="stuId" sql-type="int" not-null="true" />
		</id>
		<property name="name" type="string">
				<column name="name" sql-type="char(20)" not-null="true" />
		</property>
		<property name="message" type="string">
				<column name="message" sql-type="char(200)" not-null="false" />
		</property>
	</class>
</hibernate-mapping>